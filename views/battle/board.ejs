<div id="shogi">
  <div class="top">
    <div v-if="game.isSente" class="sente">
      <% for (var i = 1100; i <= 1106; i=i+1) { %>
        <div v-on:click="onSelect($event)" class="square" id="<%= i %>" data-koma="{{board[<%= i %>].koma}}" data-own="{{board[<%= i+j %>].isOwn}}" data-move-area="{{board[<%= i+j %>].moveArea}}">
          <div></div>
          <img v-if="board[<%= i+j %>].komaImg" v-bind:src="'/images/pieces/' + board[<%= i+j %>].komaImg" height='100%'>
        </div>
      <% } %>
    </div>
    <div v-if="game.isGote" class="gote">
      
    </div>
  </div>
  <div class="board">
    <img src="/images/parts/outBoard.png" width="100%" height="100%">
    <div v-if="game.isSente" class="sente">
      <% for (var i = 1; i <= 9; i=i+1) { %>
        <% for (var j = 90; j >= 10; j=j-10) { %>
          <div v-on:click="onSelect($event)" class="square" id="<%= i+j %>" data-koma="{{board[<%= i+j %>].koma}}" data-own="{{board[<%= i+j %>].isOwn}}" data-move-area="{{board[<%= i+j %>].moveArea}}">
            <div></div>
            <img v-if="board[<%= i+j %>].komaImg" v-bind:src="'/images/pieces/' + board[<%= i+j %>].komaImg" height='100%'>
          </div>
        <% } %>
      <% } %>
    </div>
    <div v-if="game.isGote" class="gote">
      <% for (var i = 9; i >= 1; i=i-1) { %>
        <% for (var j = 10; j <= 90; j=j+10) { %>
          <div v-on:click="onSelect($event)" class="square" id="<%= i+j %>" data-koma="{{board[<%= i+j %>].koma}}" data-own="{{board[<%= i+j %>].isOwn}}" data-move-area="{{board[<%= i+j %>].moveArea}}">
          <div></div>  
          <img v-if="board[<%= i+j %>].komaImg" v-bind:src="'/images/pieces/' + board[<%= i+j %>].komaImg" height='100%'>            
          </div>
        <% } %>
      <% } %>
    </div>
  </div>
  <div class="bottom">
  </div>
</div>